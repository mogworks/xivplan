{
  "headers": [
    {
      "source": "/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, must-revalidate"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval'; style-src 'self' 'unsafe-inline'; img-src https: data: blob:; font-src 'self' data:; connect-src 'self' https://xivstrat.cn https://*.xivstrat.cn https://xivapi.com https://*.xivapi.com https://xivcdn.com https://*.xivcdn.com; worker-src 'self' blob:;"
        },
        {
          "key": "X-Real-IP",
          "value": "{http.request.header.EO-Client-IP}"
        },
        {
          "key": "X-Client-IP",
          "value": "{http.request.header.EO-Client-IP}"
        },
        {
          "key": "X-Client-Country",
          "value": "{http.request.header.EO-Client-IPCountry}"
        }
      ]
    },
    {
      "source": "*.{png,jpg,jpeg,gif,svg,webp,avif,woff,woff2,ttf,eot,ico,mp4,webm,ogg,avi,mov}",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=604800"
        }
      ]
    },
    {
      "source": "*.{js,mjs,css}",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=604800, immutable"
        }
      ]
    },
    {
      "source": "*.xivplancn",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Content-Disposition",
          "value": "attachment"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
